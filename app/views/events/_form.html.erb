<%= form_with(model: @event, local: true) do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

    <p>
    <%= f.label :dance_type %><br />
    <%= f.text_field :dance_type %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>

    <p>
    <%= f.label :image_url %><br />
    <%= f.text_field :image_url %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>